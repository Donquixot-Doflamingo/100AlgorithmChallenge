function AlmostIncreasingSequence(arr : number[]) : boolean {
    let i = 0;
    for(i=0;i<arr.length-1;i++){
        if(arr[i]>=arr[i+1])
        {
            return false;
        }
    }
    return true;
}

console.log(AlmostIncreasingSequence([1,2,3,4,5,8,2,1]));
console.log(AlmostIncreasingSequence([1,2,3,4,5,8]));